<table class="table table-hover">
  <tr>
    <th>צבע</th>
    <th>ספק</th>
    <th>משפחה</th>
    <th>שם</th>
  </tr>
{{#each materials}}
  {{#with attributes}}
    <tr>
      <td>{{ color }}</td>
      <td><a href="#/vendor/{{ vendor }}">{{ vendor }}</a></td>
      <td><a href="#/family/{{ family }}">{{ family }}</a></td>
      <td><a href="#/material/{{ id }}">{{ name }}</a></td>
    </tr>
  {{/with}}
{{/each}}
</table>
